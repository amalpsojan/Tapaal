{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "_assets": "./src/assets",
          "_components": "./src/components",
          "_constants": "./src/components/constants",
          "_i18n": "./src/i18n",
          "_navigations": "./src/navigations",
          "_screens": "./src/screens",
          "_services": "./src/services",
          "_store": "./src/store",
          "_styles": "./src/styles",
          "_utils": "./src/utils"
        }
      }
    ]
  ]
}
